import java.io.*;
import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner scan = new Scanner(System.in);
        int N = scan.nextInt();
        int[] arr = new int[N];
        int sum = 0;
        for(int i=0;i<N;i++){
            int num = scan.nextInt();
            sum += num;
        }
        System.out.println(sum/N);
    }
}
